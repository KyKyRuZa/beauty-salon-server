{
  "name": "beauty-vite-module-monolith",
  "version": "1.0.0",
  "description": "Modular monolith architecture for beauty services platform",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "seed": "node seed_test_data.js",
    "docker-start": "node src/server.js",
    "docker-build": "docker build -t beauty-vite-app .",
    "docker-up": "docker compose up -d",
    "docker-down": "docker compose down",
    "docker-restart": "docker compose down && docker compose build app && docker compose up -d"
  },
  "keywords": [
    "modular-monolith",
    "beauty-services",
    "booking",
    "chat",
    "user-management"
  ],
  "author": "Beauty Vite Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.5",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.9.3",
    "jsonwebtoken": "^9.0.1",
    "multer": "^2.0.2",
    "pg": "^8.11.2",
    "pg-hstore": "^2.3.4",
    "redis": "^5.11.0",
    "sequelize": "^6.32.1",
    "socket.io": "^4.7.2",
    "winston": "^3.10.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "nodemon": "^3.0.1"
  },
  "overrides": {
    "tar": "^7.5.7"
  }
}
